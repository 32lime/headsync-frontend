{
  "buildCommand": "cd frontend && npm install && npx expo export:web",
  "outputDirectory": "frontend/dist",
  "devCommand": "cd frontend && npm run web",
  "installCommand": "cd frontend && npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

